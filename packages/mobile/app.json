{
  "expo": {
    "name": "Cloud Native Denmark",
    "slug": "cloudnative-denmark",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "cloudnativedenmark",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0026ce"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "dk.cloudnativedenmark.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0026ce"
      },
      "package": "dk.cloudnativedenmark.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "root": "./app"
        }
      ],
      "expo-font"
    ]
  }
}
